{
  "name": "Hassio Health Checks",
  "version": "0.1.7.1",
  "slug": "hassio_healthcheck",
  "description": "Hassio HealthChecks utils",
  "url": "https://github.com/MrRaph/hassio-addons/tree/master/traefik-proxy",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "options": {
    "BASE_DOMAIN": "hassio.local",
    "ACME_ENABLED": "false",
    "ACME_EMAIL": "",
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "BASE_DOMAIN": "str",
    "ACME_ENABLED": "str",
    "ACME_EMAIL": "str",
    "certfile": "str",
    "keyfile": "str"
  },
  "ports": {
    "5000/tcp": 5000
  },
  "docker_api": true,
  "homeassistant_api": true
}
