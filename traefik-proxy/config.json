{
  "name": "Hassio Traefik",
  "version": "0.1.6",
  "slug": "hassio_traefik",
  "description": "Packaged Traefik reverse proxy for Hassio",
  "url": "https://github.com/MrRaph/hassio-addons/tree/master/traefik-proxy",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "map": ["ssl"],
  "options": {
    "BASE_DOMAIN": "",
    "ACME_ENABLED": "true",
    "ACME_EMAIL": "",
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "BASE_DOMAIN": "str",
    "ACME_ENABLED": "str",
    "ACME_EMAIL": "str",
    "certfile": "str",
    "keyfile": "str",
  },
  "ports": {
    "8080/tcp": 8080,
    "80/tcp": 8888,
    "443/tcp": 443
  },
  "docker_api": true
}
